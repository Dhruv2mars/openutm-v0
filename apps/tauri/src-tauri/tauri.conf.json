{
  "productName": "OpenUTM",
  "version": "0.1.0",
  "identifier": "com.openutm.app",
  "build": {
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist",
    "beforeDevCommand": "",
    "beforeBuildCommand": "bunx vite build"
  },
  "app": {
    "windows": [
      {
        "title": "OpenUTM",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": "all"
  }
}
